#include <stdio.h>
#define MAXLEN 100

typedef struct {
    int element[MAXLEN];
    int top;
} stack;

stack init() {
    stack S;
    S.top = -1;
    return S;
}

int isEmpty(stack S) {
    return (S.top == -1);
}

int isFull(stack S) {
    return (S.top == MAXLEN - 1);
}

int peek(stack S) {
    if (isEmpty(S)) {
        printf("Empty\n");
        return -1;
    }
    return S.element[S.top];
}

stack push(stack S, int x) {
    if (isFull(S)) {
        printf("Full\n");
    } else {
        S.element[++S.top] = x;
        printf("Pushed %d\n", x);
    }
    return S;
}

stack pop(stack S) {
    if (isEmpty(S)) {
        printf("Empty\n");
    } else {
        printf("Popped %d\n", S.element[S.top]);
        S.top--;
    }
    return S;
}

void display(stack S) {
    if (isEmpty(S)) {
        printf("No elements\n");
    } else {
        for (int i = S.top; i >= 0; i--) {
            printf("%d ", S.element[i]);
        }
        printf("\n");
    }
}

int main() {
    int choice = 0;
    stack S = init();

    while (choice != 7) {
        printf("\n1. Push\n2. Pop\n3. Is Empty?\n4. Is Full?\n5. Display\n6. Peek\n7. Exit\n");
        printf("Choice: ");
        scanf("%d", &choice);

        switch (choice) {
            case 1: {
                int el;
                printf("Enter value: ");
                scanf("%d", &el);
                S = push(S, el);
                break;
            }
            case 2:
                S = pop(S);
                break;
            case 3:
                if (isEmpty(S)) {
                    printf("Empty\n");
                } else {
                    printf("Not empty\n");
                }
                break;
            case 4:
                if (isFull(S)) {
                    printf("Full\n");
                } else {
                    printf("Not full\n");
                }
                break;
            case 5:
                display(S);
                break;
            case 6: {
                int topElement = peek(S);
                if (topElement != -1) {
                    printf("Top: %d\n", topElement);
                }
                break;
            }
            case 7:
                printf("Exit\n");
                break;
            default:
                printf("Invalid\n");
        }
    }

    return 0;
}
